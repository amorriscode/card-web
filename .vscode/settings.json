{
    "files.exclude": {
        "src/**/*.d.ts": true,
        "src/**/*.js": true,
        "src/**/*.map": true,
        "tools/**/*.d.ts": true,
        "tools/**/*.js": true,
        "tools/**/*.map": true
    },
    "json.schemas": [
        {
            "fileMatch": [
                "config.*.json"
            ],
            "url": ".vscode/config_schema.json"
        }
    ]
}